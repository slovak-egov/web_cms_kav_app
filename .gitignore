# Ignore directories generated by Composer
/drush/contrib/
/vendor/
/web/core/
/web/modules/contrib/
/web/themes/contrib/
/web/profiles/contrib/
/web/libraries/
/config/simple_oauth_keys/

# Ignore sensitive and config information
/web/sites/*/settings.local.php
/web/sites/*/services.local.yml

# Ignore Drupal's file directory
/web/sites/*/files/

# Ignore SimpleTest multi-site environment
/web/sites/simpletest

# Ignore files generated by common IDEs
/.idea/
/.vscode/

# Ignore .env files as they are personal
/.env
/.lando.local.yml

# Ignore sql dumps
/*.sql

# Ignore storage for generated PHP code
/storage/php
